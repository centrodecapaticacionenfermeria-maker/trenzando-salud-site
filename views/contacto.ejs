
<% layout('partials/layout') %>
<h1>Contacto e inscripción</h1>
<form data-preinscripcion>
  <label>Nombre completo</label>
  <input name="nombre" required />
  <label>Correo electrónico</label>
  <input type="email" name="correo" required />
  <label>Teléfono / WhatsApp</label>
  <input name="telefono" required />
  <label>Diplomado de interés</label>
  <select name="programa" required>
    <option value="">Selecciona</option>
    <option>APS Intercultural</option>
    <option>Liderazgo Comunitario</option>
    <option>APS + Normativa</option>
  </select>
  <label>Mensaje</label>
  <textarea name="mensaje" rows="4"></textarea>
  <button class="btn" type="submit">Enviar preinscripción</button>
</form>
